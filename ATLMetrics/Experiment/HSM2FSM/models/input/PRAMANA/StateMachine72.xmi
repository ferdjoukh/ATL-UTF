    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:HSM="http://HSM/1.0"
    xsi:schemaLocation="http://HSM/1.0 HSM.ecore"
    name="sm1">
